{
  "name": "starhackit-ui",
  "title": "StarHack.It",
  "version": "5.12.0",
  "description": "React + node starter kit. A fullstack boilerplate written in ES6/7 with authentication and authorisation",
  "homepage": "https://github.com/FredericHeem/starhackit",
  "license": "NLPL",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start:prod": "http-server dist/",
    "prestart": "npm run prebuild",
    "lint": "eslint src --ext .js --ext .jsx",
    "cpd": "jscpd",
    "prebuild": "npm run lint && npm run cpd",
    "build": "webpack --config webpack.prod.js --progress",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "selenium-install": "webdriver-manager update",
    "e2e": "npm run nightwatch",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "npm run build && docker build -t starhackit/ui .",
    "docker:run": "docker run --rm -p 80:80 -p 443:443  --name starhackit-ui starhackit/ui",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "bugs": {
    "url": "https://github.com/FredericHeem/starhackit/issues"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "axios": "0.18.0",
    "debug": "3.1.0",
    "glamor": "2.20.40",
    "glamorous": "4.12.1",
    "history": "4.7.2",
    "i18next": "10.6.0",
    "i18next-browser-languagedetector": "2.1.1",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "1.5.1",
    "intl": "1.2.5",
    "lodash": "4.17.5",
    "mdlean": "0.15.0",
    "mobx": "4.1.1",
    "mobx-preact": "2.0.0",
    "preact": "8.2.7",
    "preact-compat": "3.18.0",
    "preact-deep-force-update": "0.1.0",
    "prop-types": "15.6.1",
    "qs": "6.5.1",
    "react-color": "2.14.0",
    "react-leanimate": "0.16.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.12.1",
    "segmentize": "0.4.1",
    "universal-router": "6.0.0",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "bluebird": "3.5.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chance": "1.0.13",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-jsx-extras": "1.2.1",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.1.0",
